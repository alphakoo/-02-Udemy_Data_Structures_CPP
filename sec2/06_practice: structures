} t1, t2, t3;

struct Rectangle r1;

typedef struct{

	int num;
	int cnt;
	char ch;

} Student;


int main() {
	
	struct Rectangle r10;

	Student s1;

	cout << sizeof(s1) << endl;

	Student s2 = {10,5,'A'};

	cout << s2.num << endl;
	cout << s2.cnt << endl;
	cout << s2.ch << endl;
	
	return 0;
}
